{
    "workbench.colorTheme": "GitHub Light",
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "workbench.activityBar.location": "bottom",
    "window.commandCenter": true,
    "window.titleBarStyle": "custom",
    "window.customTitleBarVisibility": "auto",
    "breadcrumbs.enabled": true,
    "activitusbar.showSourceControlCounter": true,

    "editor.fontFamily": "'Input Mono Narrow', 'Ubuntu Mono'",
    "editor.fontSize": 12,
    "editor.renderWhitespace": "boundary",
    "editor.stickyScroll.enabled": true,
    "editor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "editor.tabSize": 2,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,

    "terminal.integrated.fontFamily": "\"Input Mono Narrow\"",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.scrollback": 4096,

    "task.quickOpen.showAll": true,

    "outline.showBooleans": false,
    "outline.showArrays": false,
    "outline.showFields": false,
    "outline.showEnumMembers": false,
    "outline.showObjects": false,
    "outline.showVariables": false,

    "zenMode.centerLayout": false,
    "zenMode.hideStatusBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideActivityBar": false,

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-f>": false,
        "<C-v>": false,
        "<C-a>": false,
        "<C-n>": false,
        "<C-t>": false,
        "<C-x>": false,
        "<C-o>": false,
        "<C-i>": false,
        "<C-k>": false,
        "<C-p>": false,
        "<C-c>": false,
        "<C-l>": false,
    },
    "cSpell.userWords": [
        "datetime",
        "Hubber"
    ],
    "extensions.supportUntrustedWorkspaces": {
        "freebroccolo.reasonml": {
            "supported": true,
            "version": "1.0.38"
        },
        "vscodevim.vim": {
            "supported": true,
            "version": "1.22.0"
        },
        "grimmer.vscode-back-forward-button": {
            "supported": true,
            "version": "0.1.6"
        },
        "gruntfuggly.activitusbar": {
            "supported": true,
            "version": "0.0.46"
        },
        "ccls-project.ccls": {
            "supported": true,
            "version": "0.1.29"
        },
        "ocamllabs.ocaml-platform": {
            "supported": true
        }
    },
    "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
    "workbench.editor.centeredLayoutAutoResize": false,

    "[cpp]": {
        "editor.semanticHighlighting.enabled": false
    },
    "ccls.launch.command": "/usr/bin/ccls",
    // "ccls.cache.directory": "/mnt/Panchsheel/Steiner/Build/Index/",
    "ccls.index.threads": 16,
    "ccls.highlighting.bold.globalVariables": true,
    "ccls.highlighting.enabled.globalVariables": true,
    "ccls.highlighting.enabled.parameters": true,
    "ccls.highlighting.enabled.templateParameters": true,
    "ccls.highlighting.enabled.macros": true,
    "ccls.highlighting.enabled.enumConstants": true,
    "ccls.highlight.rainbow": 5,
    "cmake.configureOnEdit": false,
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "python.languageServer": "Pylance",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.typeCheckingMode": "basic",
    "reason.codelens.enabled": true,
    "haskell.manageHLS": "PATH",
    "omnisharp.enableDecompilationSupport": true,
    "omnisharp.enableImportCompletion": true,
    "omnisharp.enableAsyncCompletion": true,
    "omnisharp.enableMsBuildLoadProjectsOnDemand": true,
    "omnisharp.useModernNet": false,
    "omnisharp.useGlobalMono": "always",

    "workbench.settings.enableNaturalLanguageSearch": false,
    "codeium.enableSearch": true,
    "codeium.enableConfig": {
        "*": true,
        "qml": true,
        "atd": true,
        "markdown": true,
        "typescript": true,
    },
    "codeium.disableSupercomplete": true,
    "continue.telemetryEnabled": false,
    "continue.disableQuickFix": true,
    "roo-cline.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show"
    ],
    "errorLens.excludeByMessage": [
        "Unknown word"
    ],
}
